# Environment variables for AWS EC2 Production Deployment
# Database Configuration
MONGO_ROOT_USERNAME=foodhub_admin
MONGO_ROOT_PASSWORD=YOUR_SECURE_PRODUCTION_PASSWORD
MONGO_DB_NAME=reactmeals_prod

# Application URLs (replace with your EC2 public IP or domain)
FRONTEND_URL=http://YOUR_EC2_PUBLIC_IP
BACKEND_URL=http://YOUR_EC2_PUBLIC_IP:8080

# Docker Hub Configuration
DOCKER_USERNAME=your-dockerhub-username

# AWS Configuration
AWS_REGION=us-east-1
EC2_INSTANCE_TYPE=t2.micro